{
  "name": "JsonTemplate editor",
  "version": "0.1.0",
  "description": "Drag and drop jsontemplate editor",
  "repository": {
    "type": "git",
    "url": "git@github.com:rslijp/jsontemplates.git"
  },
  "scripts": {
    "serve": "webpack-dev-server --open --env=development",
    "watch": "webpack --watch -d --env development",
    "build": "webpack --env=production",
    "eslint": "node node_modules/eslint/bin/eslint.js  src/**/*.js",
    "eslint-fix": "node node_modules/eslint/bin/eslint.js  --fix src/**/*.js",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\""
  },
  "keywords": [
    "rest",
    "hateoas",
    "spring",
    "data",
    "react"
  ],
  "author": "R.Z. Slijp",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rslijp/jsontemplates/issues"
  },
  "homepage": "https://github.com/rslijp/jsontemplates",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "bootstrap": "^4.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.5.2",
    "rest": "^1.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^3.4.2",
    "date-fns": "^1.28.5",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.4",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
